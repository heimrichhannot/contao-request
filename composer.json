{
  "name": "heimrichhannot/contao-request",
  "description": "A request class, that brings the symfony/http-foundation to contao.",
  "keywords": [
    "contao",
    "symfony",
    "request"
  ],
  "type": "contao-module",
  "homepage": "https://github.com/heimrichhannot/contao-request",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Heimrich & Hannot",
      "email": "digitales@heimrich-hannot.de",
      "homepage": "http://www.heimrich-hannot.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/heimrichhannot/contao-request/issues"
  },
  "require": {
    "php": "~5.4 || ~7.0 || ^8.0",
    "contao/core-bundle": "^3.5.1 || ~4.1",
    "contao-community-alliance/composer-plugin": "~2.4 || ~3.0",
    "symfony/http-foundation": ">=2.7",
    "wa72/htmlpagedom": "^1.3 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6 || ~6.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "HeimrichHannot\\Request\\": [
        "src/"
      ]
    },
    "psr-4": {
      "HeimrichHannot\\Request\\": "src/"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HeimrichHannot\\Request\\Test\\": "tests/"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/request"
      }
    }
  }
}
